<?xml version="1.0" encoding="utf-8"?>
<MotionScene xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">
    <Transition
        app:constraintSetEnd="@+id/end"
        app:constraintSetStart="@+id/start"
        app:duration="1200"
        app:motionInterpolator="easeOut">

        <KeyFrameSet>
            <KeyAttribute
                app:framePosition="5"
                app:motionTarget="@+id/arc">
                <CustomAttribute
                    app:attributeName="sweepAngle"
                    app:customFloatValue="0" />
                <CustomAttribute
                    app:attributeName="tailLength"
                    app:customDimension="0dp" />
            </KeyAttribute>
            <KeyAttribute
                app:framePosition="30"
                app:motionTarget="@+id/arc">
                <CustomAttribute
                    app:attributeName="sweepAngle"
                    app:customFloatValue="0" />
                <CustomAttribute
                    app:attributeName="tailLength"
                    app:customDimension="100dp" />
            </KeyAttribute>
            <KeyPosition
                app:framePosition="40"
                app:keyPositionType="parentRelative"
                app:percentX="0.5"
                app:percentY="0.5"
                app:motionTarget="@+id/arc" />

            <KeyAttribute
                app:framePosition="60"
                app:motionTarget="@+id/arc">
                <CustomAttribute
                    app:attributeName="sweepAngle"
                    app:customFloatValue="280" />
                <CustomAttribute
                    app:attributeName="startAngle"
                    app:customFloatValue="43" />
                <CustomAttribute
                    app:attributeName="tailLength"
                    app:customDimension="0dp" />
            </KeyAttribute>
        </KeyFrameSet>
    </Transition>

    <Transition
        app:constraintSetEnd="@+id/back"
        app:constraintSetStart="@+id/end"
        app:duration="500">

        <KeyFrameSet>
            <KeyPosition
                app:framePosition="60"
                app:keyPositionType="parentRelative"
                app:percentX="0.5"
                app:percentY="0.5"
                app:motionTarget="@+id/arc" />

            <KeyAttribute
                app:framePosition="1"
                app:motionTarget="@+id/arc">
                <CustomAttribute
                    app:attributeName="noseLength"
                    app:customDimension="0dp" />
            </KeyAttribute>

            <KeyAttribute
                android:rotation="360"
                app:framePosition="60"
                app:motionTarget="@+id/arc">
                <CustomAttribute
                    app:attributeName="sweepAngle"
                    app:customFloatValue="0" />
            </KeyAttribute>

            <KeyAttribute
                app:framePosition="80"
                app:motionTarget="@+id/arc">

                <CustomAttribute
                    app:attributeName="noseLength"
                    app:customDimension="100dp" />
            </KeyAttribute>
        </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:id="@+id/arc"
            android:layout_width="53.5dp"
            android:layout_height="53.5dp"
            app:layout_constraintBottom_toTopOf="@+id/hiddenFab"
            app:layout_constraintEnd_toEndOf="@+id/hiddenFab"
            app:layout_constraintStart_toStartOf="@+id/hiddenFab"
            app:layout_constraintTop_toTopOf="@+id/hiddenFab"
            app:strokeColor="@color/colorAccent"
            app:strokeWidth="5dp">
            <CustomAttribute
                app:attributeName="sweepAngle"
                app:customFloatValue="0" />
            <CustomAttribute
                app:attributeName="tailLength"
                app:customDimension="0dp" />
        </Constraint>

        <Constraint
            android:id="@+id/middleGuide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />

        <Constraint
            android:id="@+id/hiddenFab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:useCompatPadding="true" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
        <Constraint
            android:id="@+id/arc"
            android:layout_width="53.5dp"
            android:layout_height="53.5dp"
            android:rotation="0"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:strokeColor="@color/colorAccent"
            app:strokeWidth="5dp">
            <CustomAttribute
                app:attributeName="sweepAngle"
                app:customFloatValue="360" />
            <CustomAttribute
                app:attributeName="startAngle"
                app:customFloatValue="232" />
        </Constraint>

        <Constraint
            android:id="@+id/middleGuide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />

        <Constraint
            android:id="@+id/hiddenFab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:useCompatPadding="true" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/back">
        <Constraint
            android:id="@+id/arc"
            android:layout_width="53.5dp"
            android:layout_height="53.5dp"
            android:rotation="360"
            app:layout_constraintBottom_toBottomOf="@+id/hiddenFab"
            app:layout_constraintEnd_toEndOf="@+id/hiddenFab"
            app:layout_constraintStart_toStartOf="@+id/hiddenFab"
            app:layout_constraintTop_toBottomOf="@+id/hiddenFab"
            app:strokeColor="@color/colorAccent"
            app:strokeWidth="5dp">
            <CustomAttribute
                app:attributeName="noseLength"
                app:customDimension="0dp" />
            <CustomAttribute
                app:attributeName="sweepAngle"
                app:customFloatValue="0" />
            <CustomAttribute
                app:attributeName="startAngle"
                app:customFloatValue="232" />
        </Constraint>

        <Constraint
            android:id="@+id/middleGuide"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            app:layout_constraintGuide_percent="0.5" />

        <Constraint
            android:id="@+id/hiddenFab"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:alpha="0"
            android:visibility="invisible"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:useCompatPadding="true" />
    </ConstraintSet>
</MotionScene>